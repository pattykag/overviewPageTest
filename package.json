{
    "name": "overviewTest",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "@sap/cds-dk": "^6.6.0",
        "@sap/xssec": "^3",
        "express": "^4",
        "hdb": "^0.19.0",
        "passport": "^0"
    },
    "devDependencies": {
        "@sap/ux-specification": "^1.108.4",
        "rimraf": "^3.0.2",
        "sqlite3": "^5.0.4"
    },
    "scripts": {
        "start": "cds w --in-memory",
        "watch-overviewmodule": "cds watch --open overviewmodule/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy overviewTest --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-statusdate": "cds watch --open statusdate/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-loglist": "cds watch --open loglist/webapp/index.html?sap-ui-xx-viewCache=false",
        "deploy-p": "npm i && mbt build && cf deploy mta_archives/CAP-overviewTest_1.0.0.mtar --retries 1"
    },
    "engines": {
        "node": "^16.15"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "auth": {
                "kind": "xsuaa"
            },
            "[production]": {
                "db": {
                    "kind": "hana-cloud"
                }
            },
            "db": {
                "kind": "sql"
            }
        }
    },
    "sapux": [
        "app/statusdate",
        "app/loglist"
    ]
}
