{
    "_version": "1.49.0",
    "sap.app": {
        "id": "nsoverviewoverview.statusdate",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:ovp",
            "version": "1.9.0",
            "toolsId": "36316074-8724-4c4d-ad86-182cab828c62"
        },
        "dataSources": {
            "mainService": {
                "uri": "/overservice/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.111.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.ovp": {},
                "sap.ui.rta": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "nsoverviewoverview.statusdate.i18n.i18n"
                }
            },
            "mainModel": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {},
            "routes": [],
            "targets": {}
        }
    },
    "crossNavigation": {
        "inbounds": {
            "intent1": {
                "signature": {
                    "parameters": {},
                    "additionalParameters": "allowed"
                },
                "semanticObject": "mycross",
                "action": "display",
                "title": "Cashapp Scheduler Bank Account Maintenance",
                "subTitle": "",
                "icon": ""
            }
        }
    },
    "sap.ovp": {
        "globalFilterModel": "mainModel",
        "globalFilterEntityType": "log_status_bankstatement_hc",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
        "chartSettings": {
            "showDataLabel": true
        },
        "cards": {
            "card00_Links": {
                "template": "sap.ovp.cards.linklist",
                "settings": {
                    "title": "Social Media",
                    "subTitle": "Contact Us",
                    "listFlavor": "standard",
                    "defaultSpan": {
                        "rows": 15,
                        "cols": 1
                    },
                    "staticContent": [
                        {
                            "title": "Google",       
                            "imageUri": "sap-icon://accelerated",
                            "targetUri": "https://www.google.com",
                            "listFlavor": "standard"
                        },
                        {
                            "title": "Twitter",       
                            "imageUri": "sap-icon://account",
                            "targetUri": "https://twitter.com/",
                            "listFlavor": "standard"
                        },
                        {
                            "title": "Instagram",       
                            "imageUri": "sap-icon://address-book",
                            "targetUri": "https://www.instagram.com/",
                            "listFlavor": "standard"
                        }
                    ]
                }
            },
            "barchart": {
                "model": "mainModel",
                "template": "sap.ovp.cards.v4.charts.analytical",
                "settings": {
                    "title": "Log Status",
                    "subTitle": "Sum of every status",
                    "entitySet": "log_status_bankstatement_hc",
                    "showDataLabel": true,
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Bar",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification#BarHeader",
                    "selectionAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#preventative",
                    "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#preventative"
                }
            },
            "mydonutcard": {
                "model": "mainModel",
                "template": "sap.ovp.cards.v4.charts.analytical",
                "settings": {
                    "title": "Status Graphic",
                    "entitySet": "log_status_bankstatement_hc",
                    "subTitle": "Donut Mode",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Donut"
                }
            },
            "lineCard": {
                "model": "mainModel",
                "template": "sap.ovp.cards.v4.charts.analytical",
                "settings": {
                    "title": "Jobs",
                    "entitySet": "log_status_bankstatement_hc",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Line"
                }
            }
        },
        "globalFilterControlType": "MacroFilterBar"
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    }
}